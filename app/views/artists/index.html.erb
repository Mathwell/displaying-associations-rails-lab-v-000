
<h1>Artists</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Song_count</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @artists.each do |artist| %>
      <tr>
        <td><%= artist.name %></td>
        <td><%=pluralize(artist.songs.count, 'Song')%></td>
      </tr>
    <% end %>
  </tbody>
</table>
